#include<stdio.h>
int main()
{
    int i;
    int j;
    int k;
    for(i=0;i<4;i++)
    {
        for(j=0;j<4;j++)
        {
            for(k=0;k<4;k++)
            {
                if(i!=j&&i!=k&j!=k)
                {
                    printf("%d%d%d\n",i,j,k);
                }
            }
        }
    }
    return 0;
}
